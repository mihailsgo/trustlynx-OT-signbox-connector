<div class="column-item-container" title="{{name}}">
  <span class="icon icon-draggable-handle6"></span>
  {{#if hasAttribute}}
  <span class="column-label" data-csui-key="{{label}}">
    <span class="csui-column-label-category">{{categoryName}}</span>
    <span class="csui-column-label-attribute">{{attributeName}}</span>
  </span>
  {{else}}
  <span class="column-label" data-csui-key="{{label}}">{{name}}</span>
  {{/if}}
</div>
{{#if isRemovable}}
  <div class="remove-button icon circle_delete" tabindex="0" role="button" aria-label="{{removeColumnAria}}"  title="{{removeColumn}}"></div>
{{/if}}
