{{#if adduserorgroup}}
  <h2 class="member-title">{{title}}</h2><span class="csui-separator"></span>
  <div class="csui-target-controls">
    {{#if locationSelector}}
      <div class="csui-start-locations binf-dropdown {{#if
          displayStartLocation}}binf-hidden{{/if}}"></div>
    {{/if}}
    <div class="csui-member-picker-container">
      <div class="csui-member-picker"></div>
    </div>
  </div>
{{else}}
  <h2 class="member-title member-picker-view-header">{{title}}</h2><span
      class="csui-view-separator csui-separator binf-hidden"></span>
  <div class="csui-target-controls">
    {{#if isNotDeleted}}
      <div class="csui-member-picker-container">
        <span class="csui-member-picker"></span>
        <span tabindex="-1" class="csui-add-member binf-hidden"
              title="{{addMemberTitle}}">
          {{{icon-v2 iconName='csui_action_add32' states='true' size='large'}}}
        </span>
      </div>
    {{/if}}
  </div>
{{/if}}
