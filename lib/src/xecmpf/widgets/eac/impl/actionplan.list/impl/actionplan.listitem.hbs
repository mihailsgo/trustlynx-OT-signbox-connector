<div class="xecmpf-eac-action-plan-list-item-selection-marker"></div>
<div class="xecmpf-eac-check-box-list"></div>
{{#if name }}
<a role="button" href="#" class="binf-flex-row xecmpf-eac-action-plan-name-link" aria-label="{{name}}" title="{{name}}" tabindex="{{tabindex}}">
    <span class="xecmpf-eac-action-plan-name">{{name}}</span>
</a>
<input id="actionplan-object-name-input-{{nodeId}}" class="actionplan-object-name-input 
    binf-form-control cs-input rename-field binf-hidden" placeholder="{{namePlaceholder}}" aria-label="{{namePlaceHolder}}" tabindex="0">
<span class="actionplan-object-name-cancel csui-undo actionplan-edit-cancel inline-edit-icon
      rename-field binf-hidden" tabindex="0" title="{{cancel}}"></span>
<div class="csui-inlineform-group csui-inlineform-group-error actionplan-error binf-hidden ">
    <div id="actionplan-object-error-message-{{nodeId}}" class="binf-text-danger actionplan-error-message">
        <span class="csui-text-danger actionplan-err-message"></span>
    </div>
</div>
{{else}}
<input type="text" class="xecmpf-eac-action-plan-list-item-input 
     binf-form-control cs-input rename-field" placeholder="{{namePlaceHolder}}" aria-label="{{namePlaceHolder}}" tabindex="0">
<span class="actionplan-object-title-cancel csui-undo actionplan-edit-cancel inline-edit-icon
      rename-field" tabindex="0" title="{{cancel}}"></span>
<div class="csui-inlineform-group csui-inlineform-group-error actionplan-create-error binf-hidden ">
    <div id="actionplan-create-object-error-message" class="binf-text-danger actionplan-create-error-message">
        <span class="csui-text-danger actionplan-create-err-message"></span>
    </div>
</div>
{{/if}}
<div class="xecmpf-eac-action-plan-actions"></div>
