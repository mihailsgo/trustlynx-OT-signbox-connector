{{#if header}}
    <div class="tile-header">

        {{#if wrNode}}
            <div class="arrow_back cs-go-back"></div>
        {{/if}}

        {{#if icon}}
            <div class="tile-type-icon">
                <span class="icon title-icon {{icon}}" aria-hidden="true"></span>
            </div>
        {{/if}}

        <div class="tile-title">
            <h2 class="csui-heading">{{title}}</h2>
        </div>

        <div class="tile-controls">
            {{#if showParameterEditBtn}}
            <div class="csui-favorite csui-acc-tab-region webreports-widget-parameters-btn">
                <span class="csui-icon-edit inline-edit-icon icon-edit wr-icon-edit" role="button" tabindex="0" aria-label="{{settings}}"></span>
            </div>
            {{/if}}
        </div>

    </div>
{{else if showParameterEditBtn}}
    <div class="csui-favorite csui-acc-tab-region webreports-widget-parameters-btn overlaid">
        <span class="csui-icon-edit inline-edit-icon icon-edit wr-icon-edit" role="button" tabindex="0" aria-label="{{settings}}"></span>
    </div>

{{/if}}



<div class="tile-content"></div>

<div class="tile-footer"></div>

